{
  "name": "finedine",
  "description": "My Portfolio project.",
  "version": "1.0.0",
  "main": "index.js",
  "scripts": {
    "watch:scss": "onchange 'css/*.scss' -- npm run scss",
    "watch:all": "parallelshell 'npm run watch:scss' 'npm run lite'",
    "imagemin": "imagemin img/* -o dist/img",
    "usemin": "usemin contact.html -d dist --htmlmin -o dist/contact.html && usemin menu.html -d dist --htmlmin -o dist/menu.html && usemin FAQs.html -d dist --htmlmin -o dist/FAQs.html && usemin index.html -d dist --htmlmin -o dist/index.html",
    "clean": "rimraf dist",
    "build": "npm run clean && npm run imagemin && npm run usemin",
    "lite": "lite-server",
    "scss": "node-sass -o css/ css/",
    "start": "npm run watch:all",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Pracasap/FineDine.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/Pracasap/FineDine/issues"
  },
  "homepage": "https://github.com/Pracasap/FineDine#readme",
  "devDependencies": {
    "copyfiles": "^2.2.0",
    "imagemin-cli": "^5.1.0",
    "lite-server": "^2.6.1",
    "node-sass": "^5.0.0",
    "onchange": "^7.0.0",
    "parallelshell": "3.0.1",
    "rimraf": "^3.0.2",
    "usemin-cli": "^0.6.0"
  },
  "dependencies": {
    "@beyonk/google-fonts-webpack-plugin": "^1.7.0",
    "bootstrap": "^4.5.2",
    "ionicons": "5.5.1",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  }
}
